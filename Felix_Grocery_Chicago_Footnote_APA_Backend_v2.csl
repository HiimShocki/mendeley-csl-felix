<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>Felix Grocery Chicago Footnote APA Backend v2</title>
    <id>http://www.zotero.org/styles/felix-grocery-chicago-footnote-apa-v2</id>
    <link href="http://www.zotero.org/styles/felix-grocery-chicago-footnote-apa-v2" rel="self"/>
    <author>
      <name>ChatGPT Custom</name>
    </author>
    <category citation-format="note"/>
    <category field="generic-base"/>
    <updated>2025-06-18</updated>
  </info>

  <macro name="author-footnote">
    <names variable="author">
      <name name-as-sort-order="all" delimiter="/" initialize-with=".">
        <name-form form="long"/>
      </name>
      <et-al term="et al." prefix=" "/>
    </names>
  </macro>

  <macro name="author-short">
    <names variable="author">
      <name name-as-sort-order="all" delimiter="/" initialize-with="." et-al-min="4" et-al-use-first="1"/>
      <et-al term="et al." prefix=" "/>
    </names>
  </macro>

  <macro name="year">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>

  <macro name="citation-locator">
    <choose>
      <if locator="page">
        <text term="page" form="short" prefix=", p. "/>
        <text variable="locator"/>
      </if>
    </choose>
  </macro>

  <citation>
    <layout delimiter=", ">
      <text value="Cf. "/>
      <names variable="author">
        <name name-as-sort-order="all" delimiter="/" initialize-with="." et-al-min="4" et-al-use-first="1"/>
        <et-al term="et al." prefix=" "/>
      </names>
      <text value=" ("/>
      <text macro="year"/>
      <text value=")"/>
      <text macro="citation-locator"/>
    </layout>
  </citation>

  <bibliography>
    <sort>
      <key variable="author"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <names variable="author">
        <name name-as-sort-order="all" delimiter="; " initialize-with="."/>
      </names>
      <text prefix=" (" suffix=")" macro="year"/>
      <text prefix=": " variable="title"/>
      <text prefix=", " variable="container-title"/>
      <group prefix=", ">
        <text variable="volume"/>
        <text prefix="(" suffix=")" variable="issue"/>
      </group>
      <text prefix=", p. " variable="page"/>
      <text prefix=", doi: " variable="DOI"/>
    </layout>
  </bibliography>
</style>
